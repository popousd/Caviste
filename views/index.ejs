<!DOCTYPE html>
<html ng-app='app'>
	<head>
		<meta charset="utf-8">
		<title>Caviste</title>
	<head>
	<body>
		<div ng-controller="RegisterController">
			<form>
				<input type="text" ng-model="user.username" placeholder="Username"><br>
				<input type="text" ng-model="user.email" placeholder="Email"><br>
				<input type="text" ng-model="user.password" placeholder="Password"><br>
				<button ng-click="register()">Register</button>
			</form> 
		</div>

		<script src ="js/angular.min.js"></script>
		<script src="app/app.module.js"></script>
	</body>
</html>